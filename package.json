{
  "name": "eidolon-server-ts",
  "version": "0.1.0",
  "description": "[![GitHub version](https://badge.fury.io/gh/abrisene%2Feidolon-server-ts.svg)](https://badge.fury.io/gh/abrisene%2Feidolon-server-ts)",
  "main": "lib/index.js",
  "author": "David Robinson <dr@monetise.io> (http://www.monetise.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abrisene/eidolon-server-ts/issues"
  },
  "homepage": "https://github.com/abrisene/eidolon-server-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abrisene/eidolon-server-ts"
  },
  "scripts": {
    "start": "npm run serve",
    "build": "npm run clean && npm run build-ts && npm run tslint",
    "serve": "node scripts/start.js",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "clean": "rm -rf ./lib && mkdir lib",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "dev": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect scripts/start.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TS,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "ably": "1.1.7",
    "airtable": "0.7.2",
    "apollo-server": "2.4.8",
    "apollo-server-cache-redis": "0.3.1",
    "apollo-server-express": "2.4.8",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.3",
    "chalk": "2.4.2",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "graphql": "14.2.1",
    "graphql-type-json": "0.3.0",
    "ioredis": "4.9.0",
    "jsonwebtoken": "8.5.1",
    "mailgun.js": "2.0.1",
    "mongoose": "5.7.9",
    "morgan": "1.9.1",
    "neo4j-driver": "1.7.3",
    "pubnub": "4.23.0",
    "pug": "2.0.3",
    "redis": "2.8.0",
    "sequelize": "5.3.1",
    "socket.io": "2.2.0",
    "stripe": "6.28.0",
    "twilio": "3.30.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "3.5.26",
    "@types/cookie-parser": "1.4.1",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/graphql": "14.2.0",
    "@types/ioredis": "4.0.10",
    "@types/jest": "24.0.11",
    "@types/jsonwebtoken": "8.3.2",
    "@types/mongoose": "5.5.30",
    "@types/morgan": "1.7.35",
    "@types/pubnub": "4.0.6",
    "@types/redis": "2.8.12",
    "@types/sequelize": "4.27.46",
    "@types/socket.io": "2.1.2",
    "@types/stripe": "6.25.10",
    "@types/twilio": "0.0.10",
    "@types/validator": "10.11.0",
    "concurrently": "4.1.0",
    "jest": "24.7.1",
    "nodemon": "1.18.10",
    "tslint": "5.15.0",
    "typescript": "3.4.2",
    "typescript-tslint-plugin": "0.3.1"
  }
}
