{
  "name": "eidolon-server-ts",
  "version": "0.1.0",
  "description": "[![GitHub version](https://badge.fury.io/gh/abrisene%2Feidolon-server-ts.svg)](https://badge.fury.io/gh/abrisene%2Feidolon-server-ts)",
  "main": "lib/index.js",
  "author": "David Robinson <dr@monetise.io> (http://www.monetise.io)",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run clean && npm run build-ts && npm run tslint",
    "serve": "node scripts/start.js",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "clean": "rm -rf ./lib && mkdir lib",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "dev": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect scripts/start.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TS,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "ably": "1.1.7",
    "airtable": "0.5.10",
    "chalk": "2.4.2",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "ioredis": "4.9.0",
    "mailgun.js": "2.0.1",
    "mongoose": "5.5.0",
    "neo4j-driver": "1.7.3",
    "pubnub": "4.23.0",
    "sequelize": "5.3.1",
    "stripe": "6.28.0",
    "twilio": "3.30.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.26",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/ioredis": "4.0.10",
    "@types/jest": "24.0.11",
    "@types/mongoose": "5.3.25",
    "@types/pubnub": "4.0.6",
    "@types/sequelize": "4.27.46",
    "@types/stripe": "6.25.10",
    "@types/twilio": "0.0.10",
    "@types/validator": "10.11.0",
    "concurrently": "4.1.0",
    "jest": "24.7.1",
    "nodemon": "1.18.10",
    "tslint": "5.15.0",
    "typescript": "3.4.2",
    "typescript-tslint-plugin": "0.3.1"
  }
}
